{
  "name": "gatway",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "concurrently -c \"cyan.bold,red.bold,white.bold,yellow.bold\" \"npm:start-*\"",
    "start-gatway": "nest start --watch gatway",
    "start-auth": "nest start --watch auth",
    "start-course": "nest start --watch course",
    "start-user": "nest start --watch user",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/gatway/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" ",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/gatway/test/jest-e2e.json"
  },
  "dependencies": {
    "@casl/ability": "^6.7.1",
    "@hapi/joi": "^17.1.1",
    "@nestjs.pro/logger-elasticsearch": "^0.1.25",
    "@nestjs/common": "^10.3.7",
    "@nestjs/core": "^10.3.7",
    "@nestjs/elasticsearch": "^10.0.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/microservices": "^10.3.7",
    "@nestjs/mongoose": "^10.0.5",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.7",
    "@nestjs/throttler": "^5.1.2",
    "@sendgrid/mail": "^8.1.3",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "concurrently": "^8.2.2",
    "dotenv": "^16.4.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "joi": "^17.12.3",
    "mongoose": "^8.3.1",
    "nest-winston": "^1.9.4",
    "node-rsa": "^1.1.1",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "tsc-watch": "^6.2.0",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.1.1",
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.7",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "flag": "^5.0.1",
    "globals": "^15.0.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.4",
    "typescript-eslint": "^7.7.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/config(|/.*)$": "<rootDir>/libs/config/src/$1",
      "^lib/helper(|/.*)$": "<rootDir>/libs/helper/src/$1",
      "^@lib/helper(|/.*)$": "<rootDir>/libs/helper/src/$1",
      "^@lib/sms(|/.*)$": "<rootDir>/libs/sms/src/$1",
      "^@lib/templates(|/.*)$": "<rootDir>/libs/templates/src/$1",
      "^@lib/logger(|/.*)$": "<rootDir>/libs/logger/src/$1",
      "^@lib/message(|/.*)$": "<rootDir>/libs/message/src/$1",
      "^@lib/responce(|/.*)$": "<rootDir>/libs/responce/src/$1",
      "^@lib/interceptor(|/.*)$": "<rootDir>/libs/interceptor/src/$1",
      "^@lib/validation(|/.*)$": "<rootDir>/libs/validation/src/$1",
      "^@lib/guard(|/.*)$": "<rootDir>/libs/guard/src/$1"
    }
  }
}
