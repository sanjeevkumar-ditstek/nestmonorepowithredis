{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/gatway/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/gatway/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "./apps/gatway",
  "projects": {
    "gatway": {
      "type": "application",
      "root": "apps/gatway",
      "entryFile": "main",
      "sourceRoot": "apps/gatway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/gatway/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "course": {
      "type": "application",
      "root": "./apps/course",
      "entryFile": "main",
      "sourceRoot": "apps/course/src",
      "compilerOptions": {
        "tsConfigPath": "apps/course/tsconfig.app.json"
      }
    },
    "config": {
      "type": "library",
      "root": "libs/config",
      "entryFile": "index",
      "sourceRoot": "libs/config/src",
      "compilerOptions": {
        "tsConfigPath": "libs/config/tsconfig.lib.json"
      }
    },
    "user": {
      "type": "application",
      "root": "apps/user",
      "entryFile": "main",
      "sourceRoot": "apps/user/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user/tsconfig.app.json"
      }
    },
    "helper": {
      "type": "library",
      "root": "libs/helper",
      "entryFile": "index",
      "sourceRoot": "libs/helper/src",
      "compilerOptions": {
        "tsConfigPath": "libs/helper/tsconfig.lib.json"
      }
    },
    "sms": {
      "type": "library",
      "root": "libs/sms",
      "entryFile": "index",
      "sourceRoot": "libs/sms/src",
      "compilerOptions": {
        "tsConfigPath": "libs/sms/tsconfig.lib.json"
      }
    },
    "templates": {
      "type": "library",
      "root": "libs/templates",
      "entryFile": "index",
      "sourceRoot": "libs/templates/src",
      "compilerOptions": {
        "tsConfigPath": "libs/templates/tsconfig.lib.json"
      }
    },
    "logger": {
      "type": "library",
      "root": "libs/logger",
      "entryFile": "index",
      "sourceRoot": "libs/logger/src",
      "compilerOptions": {
        "tsConfigPath": "libs/logger/tsconfig.lib.json"
      }
    },
    "message": {
      "type": "library",
      "root": "libs/message",
      "entryFile": "index",
      "sourceRoot": "libs/message/src",
      "compilerOptions": {
        "tsConfigPath": "libs/message/tsconfig.lib.json"
      }
    },
    "responce": {
      "type": "library",
      "root": "libs/responce",
      "entryFile": "index",
      "sourceRoot": "libs/responce/src",
      "compilerOptions": {
        "tsConfigPath": "libs/responce/tsconfig.lib.json"
      }
    },
    "interceptor": {
      "type": "library",
      "root": "libs/interceptor",
      "entryFile": "index",
      "sourceRoot": "libs/interceptor/src",
      "compilerOptions": {
        "tsConfigPath": "libs/interceptor/tsconfig.lib.json"
      }
    },
    "validation": {
      "type": "library",
      "root": "libs/validation",
      "entryFile": "index",
      "sourceRoot": "libs/validation/src",
      "compilerOptions": {
        "tsConfigPath": "libs/validation/tsconfig.lib.json"
      }
    },
    "guard": {
      "type": "library",
      "root": "libs/guard",
      "entryFile": "index",
      "sourceRoot": "libs/guard/src",
      "compilerOptions": {
        "tsConfigPath": "libs/guard/tsconfig.lib.json"
      }
    }
  }
}